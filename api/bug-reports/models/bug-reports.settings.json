{
  "kind": "collectionType",
  "collectionName": "bug_reports",
  "info": {
    "name": "BugReports",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "moneyrelated",
        "technical"
      ],
      "default": "technical"
    },
    "telegram": {
      "type": "string"
    },
    "urgency": {
      "type": "enumeration",
      "enum": [
        "high",
        "medium",
        "low"
      ],
      "default": "low"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "inprogress",
        "notstarted",
        "complete",
        "unverified"
      ],
      "default": "unverified"
    },
    "progress": {
      "type": "integer"
    },
    "Extras": {
      "type": "dynamiczone",
      "components": [
        "component.info",
        "component.attachment",
        "component.wallet-details",
        "component.location"
      ]
    }
  }
}
