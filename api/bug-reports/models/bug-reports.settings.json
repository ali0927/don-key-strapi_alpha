{
  "kind": "collectionType",
  "collectionName": "bug_reports",
  "info": {
    "name": "BugReports",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "private": true
    },
    "email": {
      "type": "email",
      "private": true
    },
    "title": {
      "type": "string"
    },
    "message": {
      "type": "text"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "moneyrelated",
        "technical"
      ],
      "default": "technical",
      "private": true
    },
    "telegram": {
      "type": "string",
      "private": true
    },
    "urgency": {
      "type": "enumeration",
      "enum": [
        "high",
        "medium",
        "low"
      ],
      "default": "low",
      "private": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "inprogress",
        "notstarted",
        "complete",
        "unverified"
      ],
      "default": "unverified"
    },
    "progress": {
      "type": "integer"
    },
    "Extras": {
      "type": "dynamiczone",
      "components": [
        "component.attachment",
        "component.wallet-details",
        "component.location"
      ]
    }
  }
}
